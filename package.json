{
  "name": "nutrio",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "8.10.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "npm run build && nodemon ./build/bin/www",
    "start-dev": "nodemon --exec babel-node ./src/bin/www",
    "clean": "rm -rf ./build && mkdir build",
    "build-babel": "babel --out-dir ./build --source-maps --copy-files ./src",
    "build": "npm run clean && npm run build-babel",
    "test": "mocha --timeout 10000 --require babel-register ./test/spoonacular_api"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "chai-arrays": "^2.0.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^5.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.0",
    "helmet": "^3.11.0",
    "hoek": "^4.2.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "serve-favicon": "~2.4.5",
    "swagger-ui-express": "^2.0.15",
    "uglify-js": "^3.4.9",
    "unirest": "^0.5.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "supertest": "^3.0.0"
  }
}
